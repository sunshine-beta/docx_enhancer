#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mr_robot/Documents/Programming/LLM-MONO/backend/node_modules/.pnpm/mammoth@1.9.1/node_modules/mammoth/bin/node_modules:/home/mr_robot/Documents/Programming/LLM-MONO/backend/node_modules/.pnpm/mammoth@1.9.1/node_modules/mammoth/node_modules:/home/mr_robot/Documents/Programming/LLM-MONO/backend/node_modules/.pnpm/mammoth@1.9.1/node_modules:/home/mr_robot/Documents/Programming/LLM-MONO/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mr_robot/Documents/Programming/LLM-MONO/backend/node_modules/.pnpm/mammoth@1.9.1/node_modules/mammoth/bin/node_modules:/home/mr_robot/Documents/Programming/LLM-MONO/backend/node_modules/.pnpm/mammoth@1.9.1/node_modules/mammoth/node_modules:/home/mr_robot/Documents/Programming/LLM-MONO/backend/node_modules/.pnpm/mammoth@1.9.1/node_modules:/home/mr_robot/Documents/Programming/LLM-MONO/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/mammoth" "$@"
else
  exec node  "$basedir/../../bin/mammoth" "$@"
fi
